   "metadata": {},
   "outputs": [],
   "source": [
    "def read_data_frames(files):\n",
    "    frames = []\n",
    "    for file in files:\n",
    "        df = pd.read_csv(file)\n",
    "        frames.append(df)\n",
    "    \n",
    "    return pd.concat(frames)"
   ]
  },
